{
    "name": "falcon758/lavaliteposts",
    "description": "Postbuffer package for channels.",
    "keywords": ["lavalite", "Postbuffer"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lavalite",
            "email": "info@lavalite.org"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds"
        ],
        "psr-4": {
            "Channels\\Postbuffer\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Channels\\Postbuffer\\Providers\\PostbufferServiceProvider"
            ],
            "aliases": {
                "Postbuffer"       : "Channels\\Postbuffer\\Facades\\Postbuffer"
            }
        }
    },
    "minimum-stability": "dev"
}
